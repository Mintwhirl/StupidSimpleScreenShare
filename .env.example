# Redis (Required)
UPSTASH_REDIS_REST_URL=your_upstash_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_token_here

# Optional: Simple authentication (recommended for personal use)
# Generate a secure random string and keep it secret
# Anyone needs this key to create rooms
AUTH_SECRET=your-random-secret-key-here

# Optional: TURN server (for strict NATs)
TURN_SERVER_URL=your-turn-server.com
TURN_SECRET=your-turn-secret-here
