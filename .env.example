# Client-side environment variables (read by browser)
VITE_PUSHER_KEY=your_key_here
VITE_PUSHER_CLUSTER=us-east-1

# Server-side environment variables (read by Vercel functions)
PUSHER_APP_ID=your_app_id_here
PUSHER_KEY=your_key_here
PUSHER_SECRET=your_secret_here
PUSHER_CLUSTER=us-east-1

# Optional: TURN server (for better NAT traversal)
TURN_SERVER_URL=turn:your-turn-server.com:443
TURN_USERNAME=username
TURN_CREDENTIAL=password
